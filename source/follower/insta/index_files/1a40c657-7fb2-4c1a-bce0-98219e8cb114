raychat_btn.setAttribute("onclick", "raychat_toggle_container()");
raychat_btn.id = "raychatBtn";
raychat_body.appendChild(raychat_btn);
Raychat.on('ready', function (e) {
    var evt = new CustomEvent('raychat_ready', {
        loadTime: new Date().getTime() - raychat_start_loading_at
    });
    window.dispatchEvent(evt);
    window.Raychat = Raychat;
});
Raychat.on('userLoaded', function (e) {
    var evt = new CustomEvent('raychat_new_user_loaded');
    window.dispatchEvent(evt);
});
